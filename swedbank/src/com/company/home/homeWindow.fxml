      <Button fx:id="addAccountButton" layoutX="470.0" layoutY="329.0" mnemonicParsing="false" onAction="#addAccount" text="Lägg till konto" />
      <Text fill="#363636" layoutX="18.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Välj en">
         <font>
            <Font size="12.0" />
         </font>
      </Text>
      <Button fx:id="logOutButton" layoutX="518.0" layoutY="13.0" mnemonicParsing="false" onAction="#logOut" text="Logg ut" />
      <Label fx:id="errorMessageBox" layoutX="185.0" layoutY="365.0" prefHeight="27.0" prefWidth="304.0" textFill="RED" />
      <Button fx:id="overforingToOthersButton" layoutX="8.0" layoutY="329.0" mnemonicParsing="false" onAction="#sendAmountToOthersAccount" text="Överföra till andra konto" />
   </children>
</AnchorPane>
