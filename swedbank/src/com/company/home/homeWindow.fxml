<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="404.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.home.HomeController">
   <children>
      <ListView fx:id="myAccountsList" layoutX="1.0" layoutY="80.0" prefHeight="193.0" prefWidth="280.0" />
      <Text layoutX="221.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Välkommen ">
         <font>
            <Font size="14.0" />
         </font></Text>
      <Label fx:id="userNameHomePage" layoutX="300.0" layoutY="15.0" prefHeight="17.0" prefWidth="97.0">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button fx:id="showTranscationsButton" layoutX="208.0" layoutY="285.0" mnemonicParsing="false" onAction="#goToTransactions" text="Visa transaktioner" />
      <ListView fx:id="transactionList" layoutX="296.0" layoutY="80.0" prefHeight="193.0" prefWidth="304.0" />
      <Text fill="#d70000" layoutX="283.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Snaste transaktioner">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Text layoutX="19.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mina konto">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button fx:id="visaMerButton" layoutX="518.0" layoutY="285.0" mnemonicParsing="false" onAction="#visaMerTransaktioner" text="Visa mer" />
      <Button fx:id="overforingButton" layoutX="6.0" layoutY="285.0" mnemonicParsing="false" onAction="#sendAmountToMyOtherAccount" text="Överföra mellan dina konton" />
